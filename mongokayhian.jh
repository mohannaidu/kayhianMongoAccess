entity ClipExtract {
	id String,
    title String,
    created_date Instant, 
    url String,
    type Integer,
    description String,
    language Integer,
    process_stage  Integer
}


// Set pagination options
paginate ClipExtract with pagination

dto * with mapstruct

// Set service options to all except few
service all with serviceImpl 
// Set an angular suffix
angularSuffix * with mySuffix
